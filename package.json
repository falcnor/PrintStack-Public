{
  "name": "printstack",
  "version": "1.8.0",
  "description": "3D Printer Inventory Management System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:analyze": "npm run build:prod && node scripts/analyze-bundle.js",
    "build:report": "npm run build:prod && node scripts/bundlephobia-check.js",
    "preview": "vite preview --port 4173",
    "preview:prod": "vite preview --config vite.prod.config.js --port 4173",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf deploy dist node_modules/.vite node_modules/.cache",
    "clean:build": "rimraf deploy dist",
    "deploy:build": "npm run clean:build && npm run build:prod",
    "deploy:check": "npm run deploy:build && npm run build:analyze",
    "optimize:images": "node scripts/optimize-images.js",
    "performance:audit": "node scripts/performance-audit.js",
    "verify-deployment": "node scripts/verify-deployment.js",
    "uptime-monitor": "node monitoring/uptime-monitor.js",
    "setup-domain": "node scripts/setup-custom-domain.cjs",
    "emergency-rollback": "bash scripts/emergency-rollback.sh",
    "deploy-emergency": "bash scripts/deploy-emergency.sh",
    "verify-rollback": "bash scripts/verify-rollback.sh",
    "test:rollback": "npm run verify-rollback && npm run uptime-monitor",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "terser": "^5.44.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.0.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11",
      "supports es2020"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
